<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="tableView" editable="true" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dice.ui.config.TableViewController">
   <columns>
      <TableColumn fx:id="nameCol" minWidth="150.0" prefWidth="150.0" text="%config_dice_collection_name_lbl" />
      <TableColumn fx:id="typeCol" minWidth="150.0" prefWidth="150.0" text="%config_dice_type_lbl" />
      <TableColumn fx:id="countCol" minWidth="150.0" prefWidth="150.0" text="%config_dice_count_lbl" />
   </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</TableView>
